{
    "name": "mongodb-backup-tool",
    "version": "1.0.0",
    "description": "Interactive tool for MongoDB backup and restoration",
    "main": "dist/index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "interactive": "tsc && node ./dist/index.js",
        "backup": "tsc && node ./dist/index.js --backup --no-interactive",
        "restore": "tsc && node ./dist/index.js --restore --no-interactive"
    },
